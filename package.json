{
  "name": "n8n-nodes-hyperliquid-hip3",
  "version": "1.0.0",
  "description": "n8n node for trading HIP3 assets on Hyperliquid",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "hyperliquid",
    "hip3",
    "crypto",
    "trading",
    "defi",
    "perpetuals"
  ],
  "license": "MIT",
  "homepage": "https://github.com/deepakdhaka-1/n8n-nodes-hyperliquid-hip3",
  "author": {
    "name": "Deepak Dhaka",
    "email": "gxyz717@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deepakdhaka-1/n8n-nodes-hyperliquid-hip3.git"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy-images",
    "copy-images": "copyfiles -u 1 \"credentials/**/*.svg\" \"nodes/**/*.svg\" dist/",
    "dev": "n8n-node dev",
    "lint": "eslint nodes credentials",
    "lint:fix": "eslint nodes credentials --fix",
    "prepare": "npm run build",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/HyperliquidApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/HyperliquidHip3/HyperliquidHip3.node.js"
    ]
  },
  "dependencies": {
    "ethers": "^5.7.2"
  },
  "devDependencies": {
    "@n8n/node-cli": "^1.0.0",
    "@types/node": "^22.0.0",
    "copyfiles": "^2.4.1",
    "typescript": "^5.3.0"
  }
}
